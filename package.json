{
  "name": "document-engine",
  "description": "Document engine for processing documents in javascript by lawing.",
  "version": "1.0.0",
  "author": "Parafern√°lia Interativa <contato@parafernalia.net.br>",
  "license": "ISC",
  "source": "src/index.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "type": "module",
  "targets": {
    "main": {
      "includeNodeModules": [
        "extenso",
        "nunjucks",
        "ordinal-pt-br",
        "slugify",
        "process"
      ]
    }
  },
  "scripts": {
    "watch": "parcel watch src/index.js",
    "build": "cross-env-shell parcel build src/index.js --out-file $npm_package_name.min.js --no-source-maps --experimental-scope-hoisting"
  },
  "dependencies": {
    "extenso": "^2.0.1",
    "nunjucks": "^3.2.3",
    "ordinal-pt-br": "^1.0.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "parcel": "^2.6.0",
    "process": "^0.11.10"
  }
}
