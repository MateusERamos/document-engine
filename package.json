{
  "name": "document-engine",
  "description": "Document engine for processing documents in javascript by lawing.",
  "version": "1.0.0",
  "author": "Parafern√°lia Interativa <contato@parafernalia.net.br>",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "cross-env-shell parcel build src/index.js --out-file $npm_package_name.min.js --no-source-maps --experimental-scope-hoisting",
    "start": "parcel serve src/index.html --open"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "nunjucks": "^3.2.3",
    "slugify": "^1.6.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write --ignore-unknown"
  }
}
